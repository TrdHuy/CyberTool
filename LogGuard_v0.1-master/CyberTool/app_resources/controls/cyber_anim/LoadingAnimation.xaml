<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:ctrl="clr-namespace:cyber_base.implement.views.cyber_anim;assembly=cyber_base">
	<Style TargetType="{x:Type ctrl:LoadingAnimation}">
		<Setter Property="ElipseFill"
				Value="{StaticResource Foreground_Level1}" />
		<Setter Property="ElipseMarginGap"
				Value="0" />
		<Setter Property="AnimationTime"
				Value="1.0" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type ctrl:LoadingAnimation}">
					<Viewbox>
						<StackPanel x:Name="PART_MainPanel"
									Orientation="Horizontal" />
					</Viewbox>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

</ResourceDictionary>