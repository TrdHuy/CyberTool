<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:local="clr-namespace:log_guard.views.others.animations">
	<Style TargetType="{x:Type local:LoadingAnimation}">
		<Setter Property="ElipseFill"
				Value="{StaticResource Foreground_Level1}" />
		<Setter Property="ElipseMarginGap"
				Value="0" />
		<Setter Property="AnimationTime"
				Value="1.0" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type local:LoadingAnimation}">
					<Viewbox>
						<StackPanel x:Name="PART_MainPanel"
									Orientation="Horizontal">

						</StackPanel>
					</Viewbox>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

</ResourceDictionary>